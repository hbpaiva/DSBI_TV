------------------------------------------

create table jaf_cat_comp_novo3 (
procedure_duration string,
msisdn string,
uri string,
mean_throughput_ul string,
mean_throughput_dl string,
total_ul_frames string,
total_dl_frames string,
total_data_ul string,
total_data_dl string,
host_name string,
cgi string,
cell_id string,
session_duration string,
categoria string
);

insert into table jaf_cat_comp_novo3
select

procedure_duration,
msisdn,
uri,
mean_throughput_ul,
mean_throughput_dl,
total_ul_frames,
total_dl_frames,
total_data_ul,
total_data_dl,
host_name,
cgi,
cell_id,
session_duration,

case

when(

    (uri like '%esoterico%'
    or uri like '%horoscopo%')

    or (host_name like '%mundomystiko.com.br%' and uri like '%%'
    or host_name like '%focusesoterismo.com.br%' and uri like '%%'
    or host_name like '%vidatarot.com.br%' and uri like '%%'
    or host_name like '%yahoo.estrelaguia.com.br%' and uri like '%%'

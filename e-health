create external table jaf_cat_portais_novo3 (
procedure_duration string,
msisdn string,
uri string,
mean_throughput_ul string,
mean_throughput_dl string,
total_ul_frames string,
total_dl_frames string,
total_data_ul string,
total_data_dl string,
host_name string,
cgi string,
cell_id string,
session_duration string,
categoria string
)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '\;'
stored as TEXTFILE


LOCATION '/home/hadoop/output/Portais_Limpo_07_01.csv';
insert into table jaf_cat_portais_novo2


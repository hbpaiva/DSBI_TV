(uri like '%products_brazalete%')
)

or 
(
(host_name like '%iarossi.com.br%')
and
(uri like '%Dentista%')
)

or
(
(host_name like '%magazineluiza.com.br%')
and
(uri like '%suplementos%')
)

or
(
(host_name like '%extra.com.br%')
and
(uri like '%Suplementos%')
)

or
(
(host_name like '%pontofrio.com.br%')
and(
(uri like '%Suplementos%')
or
(uri like '%Fitness%')
or
(uri like '%infantil%')
or
(uri like '%bebes%')
))

or
(
(host_name like '%folha.uol.com.br%')
and
(uri like '%horoscopo%')
)

or
( 
(host_name like '%celldesign.com.br%') 
and 
(uri like '%1707%')
)
;


-----------------------------------

create table jaf_cat_cronicos_novo (
procedure_duration string,
msisdn string,
uri string,
mean_throughput_ul string,
mean_throughput_dl string,
total_ul_frames string,
total_dl_frames string,
total_data_ul string,
total_data_dl string,
host_name string,
cgi string,
cell_id string,
session_duration string,
categoria string
);




insert into table jaf_cat_cronicos_novo

select

procedure_duration,
msisdn,
uri,
mean_throughput_ul,
mean_throughput_dl,
total_ul_frames,
total_dl_frames,
total_data_ul,
total_data_dl,
host_name,
cgi,
cell_id,
session_duration,

case

when(

     (uri like '%alergia%'
     or uri like '%Alergia%')
)

then 'Alergia'


when (uri like '%auto-imunes%') then 'Auto Imunes'

when (uri like '%cancer%') then 'Cancer'

when(

     (uri like '%cardiaca%'
     or uri like '%Cardiaca%'
     or uri like '%cardiaco%'
     or uri like '%cardiaco%'
     or uri like '%cardiovascular%'
     or uri like '%Cardiovascular%'
     or uri like '%coracao%')
)

then 'Cardiaco'

when (uri like '%clinica-geral%') then 'Clinica Geral'

when (uri like '%degenerativa%') then 'Degenerativa'

when(

   (uri like '%diabete%')

   or (host_name like '%adj.org.br%' and uri like '%%'
   or host_name like '%blogdasaude.com.br%' and uri like '%categorias/saude-fisica%'
   or host_name like '%diabetenet.com.br%' and uri like '%%'
   or host_name like '%diabete.com.br%' and uri like '%%'
   or host_name like '%diabetesedesportes.com.br%' and uri like '%%'
   or host_name like '%portaldiabetes.com.br%' and uri like '%%')
)

then 'Diabetes'

when (host_name like '%drauziovarella.com.br%' AND uri like '%/category/homem-2/%') then 'Doenca Homem'

when (host_name like '%drauziovarella.com.br%' AND uri like '%/category/mulher-2/%') then 'Doenca Mulher'

when (uri like '%doencas-de-pele%') then 'Doenca Pele'

when (uri like '%doenca%') then 'Doenca'

when(

   (uri like '%exame%'
   or uri like '%diagnostico%')
)

then 'Exames'

when(

    (uri like '%fertilidade%'
    or uri like '%periodo-fertil%'
    or uri like '%ovulacao%')
)

then 'fertilidade'

when (uri like '%disturbios-gastrointestinais%') then 'Gastro Intestinais'

when(

    (uri like '%filhos%'
    or uri like '%gravidez%'
    or uri like '%Gravidez%'
    or uri like '%gravida%'
    or uri like '%Gravida%'
    or uri like '%Gestante%'
    or uri like '%gestante%'
    or uri like '%gestacao%'
    or uri like '%Gestacao%' 
    or uri like '%familia%' 
    or uri like '%amamentacao%'
    or uri like '%engravida%'
    or uri like '%idade-gestacional%'
    or uri like '%fecunda%'
    or uri like '%leite-materno%'
    or uri like '%bebe%')

   or (host_name like '%bebe.abril.com.br%' AND uri like '%%') 
)

then 'Gravidez'


when (uri like '%doencas-hormonais%') then 'Hormonais'


when(

     (uri like '%doencas-infecciosas%'
     or uri like '%virus%'
     or uri like '%bacteria%')
)

then 'Infecciosas'

when(

     (uri like '%nutricao%'
     or uri like '%Nutricao%')
)

then 'Nutrição'

when (uri like '%obesidade%') then 'Obesidade'

when (uri like '%crianca%') then 'Pediatria'

when (uri like '%primeiros-socorros%') then 'Primeiros Socorros'

when (uri like '%psicologicos%') then 'Psicológicos'

when(

    (uri like '%doencas-respiratorias%'
    or uri like '%respirar%'
    or uri like '%pneumonia%'
    or uri like '%asma%')
)

then 'Respiratórias'

when(

    (uri like '%aparelho-reprodutor%'
    or uri like '%utero%'
    or uri like '%ovario%')
) 

then 'Reprodutor'

when (uri like '%disturbios-do-sangue%') then 'Sangue'

when (uri like '%corpo-humano%') then 'Saúde'

when(

    (uri like '%sexualidade%'
    or uri like '%anticoncepcional%'
    or uri like '%pilula-do-dia-seguinte%')
) 

then 'Sexualidade'

when (uri like '%sintomas%') then 'Sintomas'

when (uri like '%disturbios-do-sono%') then 'Sono'

when(

    (uri like '%tabagismo%'
    or uri like '%cigarro%'
    or uri like '%fumo%'
    or uri like '%antitabagismo%'
    or uri like '%fumante%'
    or uri like '%nicotina%'
    or uri like '%fumar%')

    or (host_name like '%deixardefumar.com.br%' and uri like '%%'
    or host_name like '%easywaysp.com.br%' and uri like '%%'
    or host_name like '%euvouparardefumar.com%' and uri like '%%')
)

then 'Tabagismo'

when(

    (host_name like '%tuasaude.com%' and uri like '%%'
    or host_name like '%drauziovarella.com.br%' and uri like '%%' 
    or host_name like '%bulas.med.br%' and uri like '%%'
    or host_name like '%blogdasaude.com.br%' and uri like '%%'
    or host_name like '%tourlife.com.br%' and uri like '%%'
    or host_name like '%medclick.com.br%' and uri like '%%'
    or host_name like '%saudecominteligencia.com.br%' and uri like '%%'
    or host_name like '%blogdasaude.com.br%' and uri like '%%'
    or host_name like '%criasaude.com.br%' and uri like '%%')
)

then 'Portal'

END AS Categoria from tbgdt_stg_dpi


where ((msisdn <> 'NA' or msisdn is not null or length(msisdn)>0)
     and  (host_name <> 'NA' or host_name is not null or length(host_name)>0))

and(
(host_name like	'%adj.org.br%')
or (host_name like '%blogdasaude.com.br%')
or (host_name like '%bulas.med.br%')
or (host_name like '%criasaude.com.br%')
or (host_name like '%deixardefumar.com.br%')
or (host_name like '%diabete.com.br%')
or (host_name like '%diabetenet.com.br%')
or (host_name like '%diabetesedesportes.com.br%')
or (host_name like '%drauziovarella.com.br%')
or (host_name like '%easywaysp.com.br%')
or (host_name like '%euvouparardefumar.com%')
or (host_name like '%medclick.com.br%')
or (host_name like '%portaldiabetes.com.br%')
or (host_name like '%saudecominteligencia.com.br%')
or (host_name like '%tourlife.com.br%')
or (host_name like '%tuasaude.com%')
);

------------------------------------------ Salvar no hadoop a tabela agregada ------------------------

insert overwrite local directory './output'  ROW FORMAT DELIMITED FIELDS TERMINATED BY '\177' select msisdn, categoria, count (*), sum(procedure_duration), sum(total_data_dl), sum(session_duration) from jaf_cat_comp_novo group by categoria, msisdn SORT BY msisdn ASC;

insert overwrite local directory './output'  ROW FORMAT DELIMITED FIELDS TERMINATED BY '\177' select msisdn, categoria, count (*), sum(procedure_duration), sum(total_data_dl), sum(session_duration) from jaf_cat_cronicos_novo group by categoria, msisdn SORT BY msisdn ASC;

insert overwrite local directory './output'  ROW FORMAT DELIMITED FIELDS TERMINATED BY '\177' select msisdn, categoria, count (*), sum(procedure_duration), sum(total_data_dl), sum(session_duration) from jaf_cat_portais_novo group by categoria, msisdn SORT BY msisdn ASC;

-------------------------------- consolida o resultado em R -----------------------

setwd("/home/h_loadbd/output")

## Ler o Arquivo Portais tirando a notação cientifica / Subset por acesso, procedure duration e total_data_dl/reshape - modelar a tabela na forma correta


portagg<-read.table("Portais Agregado.txt", sep="\177")
portagg<-format(portagg,scientific=FALSE)

port_acesso<-subset(portagg, select = c("V1","V2","V3"))

port_procedure_duration<-subset(portagg, select = c("V1","V2","V4"))

port_total_data_dl<-subset(portagg, select = c("V1","V2","V5"))

tab_port_acesso<-reshape(port_acesso, direction = "wide", idvar="V1", timevar="V2")
tab_port_procedure_duration<-reshape(port_procedure_duration, direction = "wide", idvar="V1", timevar="V2")
tab_port_total_data_dl<-reshape(port_total_data_dl, direction = "wide", idvar="V1", timevar="V2")


write.table(tab_port_acesso,file="tab_port_acesso",sep="\177",row.names=FALSE)
write.table(tab_port_procedure_duration,file="tab_port_procedure_duration",sep="\177",row.names=FALSE)
write.table(tab_port_total_data_dl,file="tab_port_total_data_dl",sep="\177",row.names=FALSE)


## Ler Arquivo Cron


cronagg<-read.table("Cronicos Agregado.txt", sep="\177")
cronagg<-format(cronagg,scientific=FALSE)

cron_acesso<-subset(cronagg, select = c("V1","V2","V3"))

cron_procedure_duration<-subset(cronagg, select = c("V1","V2","V4"))

cron_total_data_dl<-subset(cronagg, select = c("V1","V2","V5"))


tab_cron_acesso<-reshape(cron_acesso, direction = "wide", idvar="V1", timevar="V2")
tab_cron_procedure_duration<-reshape(cron_procedure_duration, direction = "wide", idvar="V1", timevar="V2")
tab_cron_total_data_dl<-reshape(cron_total_data_dl, direction = "wide", idvar="V1", timevar="V2")

write.table(tab_cron_acesso,file="tab_cron_acesso",sep="\177",row.names=FALSE)
write.table(tab_cron_procedure_duration,file="tab_cron_procedure_duration",sep="\177",row.names=FALSE)
write.table(tab_cron_total_data_dl,file="tab_cron_total_data_dl",sep="\177",row.names=FALSE)


## Ler Arquivo Competidores


compagg<-read.table("Competidores Agregado.txt", sep="")
compagg<-format(compagg,scientific=FALSE)

comp_acesso<-subset(compagg, select = c("V1","V2","V3"))

comp_procedure_duration<-subset(compagg, select = c("V1","V2","V4"))

comp_total_data_dl<-subset(compagg, select = c("V1","V2","V5"))



tab_comp_acesso<-reshape(comp_acesso, direction = "wide", idvar="V1", timevar="V2")
tab_comp_procedure_duration<-reshape(comp_procedure_duration, direction = "wide", idvar="V1", timevar="V2")
tab_comp_total_data_dl<-reshape(comp_total_data_dl, direction = "wide", idvar="V1", timevar="V2")

write.table(tab_comp_acesso,file="tab_comp_acesso",sep="\177",row.names=FALSE)
write.table(tab_comp_procedure_duration,file="tab_comp_procedure_duration",sep="\177",row.names=FALSE)
write.table(tab_comp_total_data_dl,file="tab_comp_total_data_dl",sep="\177",row.names=FALSE)

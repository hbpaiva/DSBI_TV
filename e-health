    or host_name like '%universofeminino%' and uri like '%%'
    or host_name like '%mulher.yahoo.com%' and uri like '%%')
)

then 'Mulher'

when host_name like '%deles.ig.com.br%' then 'Homem'

when( 

(host_name like '%saude.terra.com.br%' AND uri like '%%'

or host_name like '%r7.minhavida.com.br%' AND uri like '%%' 

or host_name like '%minhavida.com.br%' AND uri like '%%' 


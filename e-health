     or host_name like '%osascodagente.com%')
)

then 'Saude Bucal'


when(

    (uri like '%beleza%'
    or uri like '%moda-e-beleza%')

    or (host_name like '%beleza.terra.com.br%' AND uri like '%%'
    or host_name like '%belezanaweb.com.br%' and uri like '%%'
    or host_name like '%novidadesdebeleza.com.br%' and uri like '%%'
    or host_name like '%beautyrock.com.br%' and uri like '%%'
    or host_name like '%belezainteligente.com.br%' and uri like '%%'
    or host_name like '%euacreditoemcosmeticos.com%' and uri like '%%'
    or host_name like '%mulherbeleza.com.br%' and uri like '%%'
    or host_name like '%msn.com%' and uri like '%estilo-de-vida%')

)

then 'Beleza'

when(

    (uri like '%mulher%'
    or uri like '%Mulher%'
    or uri like '%mulheres%')

    or (host_name like '%mulher.uol.com.br%' and uri like '%%'
    or host_name like '%mdemulher.abril.com.br%' AND uri like '%%'
    or host_name like '%delas.ig.com.br%' AND uri like '%%'

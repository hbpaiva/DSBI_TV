DSBI_TV
=======

duracao<- function(x,colA,colB,colT){ 
nl=nrow(x)
f=1
j=1
v=1
p=integer()
tempo=integer()
ocorrencias=integer()
while(j <= nl){
		  k=1
		  a<-x[j,colA]
		  b<-x[j,colB]
		  c<-x[j,colT]
		  for(i in j:nl){
					if(x[i,colA]==a){
							if(x[i,colB]==b){
									c[k]<-x[i,colT]
									k=k+1
																			
		}
		}
		}		
		 tempo[f]<-sum(c)
		 ocorrencias[f]<-k-1
		 f=f+1
		 j=j+1	
		 }	
return(data.frame(tempo,ocorrencias))
}
